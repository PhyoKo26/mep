{
  "name": "MEP",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "mep:uat": "yarn clean && yarn android",
    "mep:prod": "yarn clean && yarn android:prod",
    "clean": "rm -rf $TMPDIR/react-* $TMPDIR/metro-*",
    "preandroid": "cross-env APP_ENV=uat node set-env.js",
    "android": "react-native run-android",
    "preandroid:prod": "cross-env APP_ENV=prod node set-env.js",
    "android:prod": "react-native run-android",
    "start": "react-native start",
    "ios": "react-native run-ios",
    "start:uat": "ENVFILE=.env.uat react-native start",
    "ios:uat": "ENVFILE=.env.uat react-native run-ios",
    "start:prod": "ENVFILE=.env.prod react-native start",
    "ios:prod": "ENVFILE=.env.prod react-native run-ios",
    "link": "react-native link",
    "font": "npx react-native-asset",
    "pod-install": "npx pod-install",
    "watchman-clear": "watchman watch-del-all",
    "lint": "eslint .",
    "start:clean": "yarn start --reset-cache",
    "test": "jest",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx}\"",
    "react-devtools": "react-devtools"
  },
